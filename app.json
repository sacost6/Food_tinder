{
  "expo": {
    "name": "UPick",
    "slug": "first_app",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/upick_icon_2.png",
    "splash": {
      "image": "./assets/upick_logo_v2.png",
      "resizeMode": "contain",
      "backgroundColor": "#1c1c1c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.demboiz.upick"
    },
    "android": {
      "package": "com.demboiz.upick",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION "
      ],
      "versionCode": 2
    }
  }
}
